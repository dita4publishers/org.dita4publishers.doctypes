   <div>
      <a:documentation>Specialization attributes. Global attributes and class defaults</a:documentation>
      <define name="epigram.attlist" combine="interleave">
         <ref name="global-atts"/>
         <optional>
            <attribute name="class"
                       a:defaultValue="+ topic/p          d4p-pubcontent-d/epigram "/>
         </optional>
      </define>
      <define name="pullquote.attlist" combine="interleave">
        <ref name="global-atts"/>
