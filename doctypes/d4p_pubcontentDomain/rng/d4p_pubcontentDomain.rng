         </define>
         <define name="epigram.element">
            <element name="epigram" dita:longName="epigram">
               <a:documentation>docs go here.</a:documentation>
               <ref name="epigram.attlist"/>
               <ref name="epigram.content"/>
            </element>
         </define>
         <define name="epigram.attlist" combine="interleave">
            <ref name="epigram.attributes"/>
         </define>
      </div>
