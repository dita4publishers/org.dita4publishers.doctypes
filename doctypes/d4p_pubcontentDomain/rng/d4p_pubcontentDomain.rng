      </define>
      <define name="d4pAssetSource">
        <ref name="d4pAssetSource.element"/>
      </define>
      <define name="d4pAttributedQuote">
        <ref name="d4pAttributedQuote.element"/>
      </define>
      <define name="d4pQuoteAttribution">
        <ref name="d4pQuoteAttribution.element"/>
      </define>
      <define name="epigram">
         <ref name="epigram.element"/>
      </define>
      <define name="epigraph">
         <ref name="epigraph.element"/>
      </define>
      <define name="epigraph-attribution">
         <ref name="epigraph-attribution.element"/>
      </define>
      <define name="pullquote">
        <ref name="pullquote.element"/>
      </define>
      <define name="section-pullquote">
         <ref name="section-pullquote.element"/>
      </define>
   </div>
