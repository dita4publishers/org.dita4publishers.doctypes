              <valueMapEntry>
                <valueKey>key1</valueKey>
                <optionValue>value 1</optionValue>
              </valueMapEntry>
              <valueMapEntry>
                <valueKey>key2</valueKey>
                <optionValue>value 2</optionValue>
              </valueMapEntry>
              <valueMapEntry>
                <valueKey>key3</valueKey>
                <optionValue>value 3</optionValue>
              </valueMapEntry>
            </valueMap>
          </optionValueMap>
        </option>
      </optionSet>
    </xml2InDesignOptions>
    <otherOptions>
      <optionSet>
        <option>
          <optionName>option1</optionName>
          <optionValue>option value</optionValue>
        </option>
      </optionSet>
    </otherOptions>
